
{{#if model.0.UserId }}
    <h1>Confirm delete {{model.0.UserLogin}}</h1>


    <form action="/users/delete" method="post">
        <input type="hidden" id="UserId" name="UserId" value="{{model.0.UserId}}">
        <input type="Submit" value="Confirm">
        <a href="#" onclick="history.back()">Cancel</a>
    </form>
{{else}}
    <h1>No user found</h1>
{{/if}}
